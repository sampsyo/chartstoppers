---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:sy="http://purl.org/rss/1.0/modules/syndication/" xmlns:admin="http://webns.net/mvcb/" xmlns:atom="http://www.w3.org/2005/Atom/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd">
  <channel>
    <title>{{ site.name }}</title>
    <link>{{ site.url }}</link>
    <pubDate>{{ site.time | date_to_xmlschema }}</pubDate>
    <description>{{ site.podcast.description }}</description>
    <language>en-US</language>
    <copyright>{{ site.podcast.copyright }}</copyright>
    <itunes:author>{{ site.podcast.author }}</itunes:author>
    <itunes:keywords>{{ site.podcast.keywords }}</itunes:keywords>
    <itunes:explicit>{{ site.podcast.explicit }}</itunes:explicit>
    <itunes:image href="{{ site.url }}{{ site.base }}{{ site.podcast.image }}"/>
    <itunes:owner>
      <itunes:name>{{ site.podcast.author }}</itunes:name>
      <itunes:email>{{ site.podcast.email }}</itunes:email>
    </itunes:owner>
    <itunes:block>no</itunes:block>
    <itunes:category text="{{ site.podcast.category }}"/>
    {% for ep in site.eps %}
    <item>
      <title>{{ ep.title }}</title>
      <description>{{ ep.content | xml_escape }}</description>
      <pubDate>{{ ep.date | date_to_xmlschema }}</pubDate>
      <enclosure url="{{ site.url }}{{ site.base }}audio/{{ ep.number }}.mp3" length="{{ ep.audiolength }}" type="audio/mp3"/>
      <link>{{ site.url }}{{ site.base }}{{ ep.number }}</link>
      <guid>{{ site.url }}{{ site.base }}{{ ep.number }}</guid>
      <itunes:author>{{ site.podcast.author }}</itunes:author>
      <itunes:subtitle>{{ ep.content | xml_escape }}</itunes:subtitle>
      <itunes:summary>{{ ep.content | xml_escape }}</itunes:summary>
      <itunes:explicit>{{ site.podcast.explicit }}</itunes:explicit>
      <itunes:duration>{{ ep.audiolength }}</itunes:duration>
      <content:encoded>{{ ep.content | xml_escape }}</content:encoded>
      <itunes:image href="{{ site.url }}{{ site.base }}{{ site.podcast.image }}"/>
    </item>
    {% endfor %}
  </channel>
</rss>
